{
  "expo": {
    "name": "SmartDoctor",
    "slug": "SmartDoctor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mysplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/myicon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.xfrh.SmartDoctor"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
 
    "extra": {
      "eas": {
        "projectId": "727be536-d4a2-4d73-9a61-f39181f1d563"
      }
    },
    "plugins": [
      [
        "./plugins/trust-local-certs.js"

      ]
    ]
  

  }
}
